<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :song_artist_name%>
  <%= f.text_field :artist_name%>
  <%= f.label :song_genre_id%>
  <%= f.collection_select :genre_id, Genre.all, :id, :name%>
  <input name= "song[notes][]" id = "song_notes_1">
  <input name= "song[notes][]" id = "song_notes_2">
  <br />

  <%= f.submit %>
<% end %>


